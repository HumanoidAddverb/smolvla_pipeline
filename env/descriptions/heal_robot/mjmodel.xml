<mujoco model="heal">
   <option gravity="0 0 -4.0" />
   <compiler angle="radian" 
   inertiafromgeom="false" 
   discardvisual="false" 
   meshdir="meshes/" />
<!-- 
   <statistic center="0.3 0 0.4" extent="1.5"/> -->

   <asset>
      <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="100"/>
      <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300"/>
      <material name="arm" rgba="0.75294 0.75294 0.75294 0"/>
      
      <mesh name="base_link" file="base_link.STL" />
      <mesh name="link1" file="link_1.STL" />
      <mesh name="link2" file="link_2.STL" />
      <mesh name="link3" file="link_3.STL" />
      <mesh name="link4" file="link_4.STL" />
      <mesh name="link5" file="link_5.STL" />
      <mesh name="end_effector" file="end_effector.STL" />
   </asset>

   <worldbody>
      
      <!-- Arm -->
      <body name="base_link" pos="0.0 0 0" euler="0 0 0">
         
         <geom name="base_link_visual" type="mesh" mesh="base_link" group="1" contype="0" conaffinity="0"/>
         <geom name="base_link_collision" type="mesh" mesh="base_link" group="0" contype="0" conaffinity="0" rgba="0.79216 0.81961 0.93333 1"/>
         <body name="link_1" pos="0 0 0.171">
            <inertial pos="-12.54E-03 -0.04E-03 111.27E-03" 
                      quat="0.704455 -0.0304055 0.0162858 0.70891" 
                      mass="7.92" diaginertia="0.00490984 0.00426023 0.00389513" />
            <joint name="joint_1" pos="0 0 0" axis="0 0 1" range="-3.14159 3.14159" damping="10" frictionloss="0.1" actuatorfrcrange="-10000 10000" />
            <geom name="link_1_visual" type="mesh" mesh="link1" group="1" contype="0" conaffinity="0"  rgba="0.79216 0.81961 0.93333 1"/>
            <geom name="link_1_collision" type="mesh" mesh="link1" group="0"/>
            
            <body name="link_2" pos="0 0.0875 0.1498" quat="0.707105 0.707108 0 0">
               <inertial pos="-0.00017021 0.1561 -0.016973" 
                         quat="0.499892 0.50026 -0.499813 0.500035" 
                         mass="1.24" diaginertia="0.0097684 0.0086581 0.0014536" />
               <joint name="joint_2" pos="0 0 0" axis="0 0 -1" range="-3.14159 3.14159" damping="40" frictionloss="0.1" actuatorfrcrange="-10000 10000" />
               <geom name="link_2_visual" type="mesh" mesh="link2" group="1" contype="0" conaffinity="0"  rgba="0.79216 0.81961 0.93333 1"/>
               <geom name="link_2_collision" type="mesh" mesh="link2" group="0"/>
               
               <body name="link_3" pos="0 0.3 0" euler="0 0 -1.57">
                  <inertial pos="-7.5516e-07 0 0.073251" 
                            quat="0.495149 0.509997 -0.506629 0.487912" 
                            mass="5.57" diaginertia="0.0048786 0.00425304 0.00369796" />
                  <joint name="joint_3" pos="0 0 0" axis="0 0 1" range="-3.14159 3.14159" damping="10" frictionloss="0.1" actuatorfrcrange="-10000 10000" />
                  <geom name="link_3_visual" type="mesh" mesh="link3" group="1" contype="0" conaffinity="0"  rgba="0.79216 0.81961 0.93333 1"/>
                  <geom name="link_3_collision" type="mesh" mesh="link3" group="0"/>

                  <body name="link_4" pos="0 0.1593 0.0875" euler="-1.57 0 0">
                     <inertial pos="1.3303e-05 0.06 0.10412" 
                               quat="0.313668 0.63349 -0.634256 0.313086" 
                               mass="1.9" diaginertia="0.00162547 0.0014257 0.00104183" />
                     <joint name="joint_4" pos="0 0 0" axis="0 0 1" range="-3.14159 3.14159" damping="10"  frictionloss="0.1" actuatorfrcrange="-10000 10000" />
                     <geom name="link_4_visual" type="mesh" mesh="link4" group="1" contype="0" conaffinity="0" rgba="0.79216 0.81961 0.93333 1"/>
                     <geom name="link_4_collision" type="mesh" mesh="link4" group="0"/>

                     <body name="link_5" pos="0 0.03185 0.16105" euler="0.50951 0 1.57">
                        <inertial pos="-5.6194e-05 -0.046 0.059696" 
                                  quat="0.457117 0.537094 0.438555 0.556994" 
                                  mass="1.78" diaginertia="0.000913478 0.000827593 0.000774169" />
                        <joint name="joint_5" pos="0 0 0" axis="0 0 1" range="-3.14159 3.14159" damping="10"  frictionloss="0.1" actuatorfrcrange="-10000 10000" />
                        <geom name="link_5_visual" type="mesh" mesh="link5" group="1" contype="0" conaffinity="0" rgba="0.79216 0.81961 0.93333 1"/>
                        <geom name="link_5_collision" type="mesh" mesh="link5" group="0"/>

                        <body name="end_effector" pos="0 -0.1227 0.0654" quat="0.707105 0.707108 0 0">
                           <body name="right_hand" pos="0.0 0.0 0.0" quat="1 0 0 0">
                              <camera mode="fixed" name="right_eye_in_hand" pos="0.05 0 0" quat="0.5 0.5 -0.5 -0.5" fovy="75"/>
                           </body>
                           <inertial pos="7.0245e-06 -0.00022138 -0.016847" 
                                     quat="-0.0128774 0.707029 0.00995758 0.706998" 
                                     mass="0.001" diaginertia="0.000326122 0.000191249 0.000189678" />
                           <joint name="joint_6" pos="0 0 0" axis="0 0 -1" range="-3.14159 3.14159" damping="10"  frictionloss="0.1" actuatorfrcrange="-10000 10000" />
                           <site name="right_center" pos="0 0 0" size="0.01" rgba="1 0.3 0.3 1" group="2"/>
                           <geom name="end_effector_visual" type="mesh" mesh="end_effector" group="1" contype="0" conaffinity="0" rgba="0.79216 0.81961 0.93333 1"/>
                           <geom name="end_effector_collision" type="mesh" mesh="end_effector" group="0"/>
                              <site name="attachment_site" pos="0 0 0" quat="1 0 0 0"/>
                              <camera name="wrist_cam" pos="0.0 0.07 -.05 " quat="0 0 1 0" />
                        </body>
                     </body>
                  </body>   
               </body>
            </body>
         </body>
      </body>
   </worldbody>
   <!-- Actuators for Left Arm -->
  <actuator>
      <position name="turret" joint="joint_1" kp="500" ctrlrange="-3.14159 3.14159"/>
      <position name="shoulder" joint="joint_2" kp="500" ctrlrange="-3.14159 3.14159"/>
      <position name="elbow" joint="joint_3" kp="500" ctrlrange="-3.14159 3.14159"/>
      <position name="wrist_1" joint="joint_4" kp="500" ctrlrange="-3.14159 3.14159"/>
      <position name="wrist_2" joint="joint_5" kp="500" ctrlrange="-3.14159 3.14159"/>
      <position name="wrist_3" joint="joint_6" kp="500" ctrlrange="-3.14159 3.14159"/>
  </actuator>

   <!-- <actuator>
      <velocity name="turret"    joint="joint_1" />
      <velocity name="shoulder"  joint="joint_2" />
      <velocity name="elbow"     joint="joint_3" />
      <velocity name="wrist_1"   joint="joint_4" />
      <velocity name="wrist_2"   joint="joint_5" />
      <velocity name="wrist_3"   joint="joint_6" />
   </actuator> -->

</mujoco>
