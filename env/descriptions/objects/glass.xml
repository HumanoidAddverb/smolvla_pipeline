<mujoco model="mesh_model">
<!-- <option gravity="0 0 -9.81"/> -->
  <asset>
    <!-- Add your mesh file here -->
    <mesh name="your_mesh" file="assets/white_mesh.stl" scale="0.07 0.07 0.07"/>
  </asset>
  
  <default>
    <default class="mesh_object">
      <!-- Changed from box to mesh -->
      <geom type="mesh" mesh="your_mesh" rgba="0.96 0.26 0.33 1.0" density="1240" solref="0.004 1" contype="3" group="1"/>
      <site type="sphere" size=".005" rgba="0 0 1 1" group="5"/>
    </default>
  </default>
  
  <worldbody>
    <!-- <body name="object_0" pos="0.3 0 .021" quat="0.5 0.5 0.5 0.5">
      <freejoint name="object_joint_0"/>
      <inertial pos="0 0 0" mass="0.01" diaginertia="0.0001 0.0001 0.0001"/>
      <geom name="object_0" class="mesh_object"/>
      <site name="com_0" group="5"/>
    </body> -->

    <geom name="table" type="box" size="0.4 0.25 0.005" pos="0.0 -0.4 0.2" rgba="0.7 0.5 0.3 1"/>

    <body name="hollow_bin" pos="-0.15 -0.3 0.21">
      <geom name="bin_bottom" type="box" size="0.1 0.1 0.002" pos="0 0 0" rgba="0.2 0.8 0.6 1"/>
      
      <!-- Front wall -->
      <geom name="bin_front" type="box" size="0.1 0.002 0.02" pos="0 0.098 0.02" rgba="0.2 0.8 0.6 1"/>
      
      <!-- Back wall -->
      <geom name="bin_back" type="box" size="0.1 0.002 0.02" pos="0 -0.098 0.02" rgba="0.2 0.8 0.6 1"/>
      
      <!-- Left wall -->
      <geom name="bin_left" type="box" size="0.002 0.1 0.02" pos="-0.098 0 0.02" rgba="0.2 0.8 0.6 1"/>
      
      <!-- Right wall -->
      <geom name="bin_right" type="box" size="0.002 0.1 0.02" pos="0.098 0 0.02" rgba="0.2 0.8 0.6 1"/>
    </body>


    <body name="cube_blue" pos="0.2 -0.4 0.22" quat="0.5 0.5 0.5 0.5">
      <freejoint name="cube_joint_0"/>
      <inertial pos="0 0 0" mass="0.01" diaginertia="0.0001 0.0001 0.0001"/>
      <geom name="cube_blue" type="box" size="0.02 0.02 0.02" rgba="0.2 0.6 0.2 1"/>
    </body>

    <!-- <body name="cube_red" pos="0.3 -0.4 0.22" quat="0.5 0.5 0.5 0.5">
      <freejoint name="cube_joint_1"/>
      <inertial pos="0 0 0" mass="0.01" diaginertia="0.0001 0.0001 0.0001"/>
      <geom name="cube_red" type="box" size="0.02 0.02 0.02" rgba="0.8 0.6 0.1 1"/>
    </body> -->
  </worldbody>
</mujoco>